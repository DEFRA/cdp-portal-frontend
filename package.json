{
  "name": "cdp-portal-frontend",
  "version": "1.0.0",
  "description": "CDP Portal Frontend",
  "license": "OGL-UK-3.0",
  "author": "Defra DDTS",
  "sideEffects": false,
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "run-s build:client",
    "build:client": "NODE_ENV=production webpack",
    "client:watch": "webpack --watch",
    "client:watch:debug": "DEBUG=true webpack --watch",
    "predev": "npm run build:client",
    "dev": "NODE_ENV=development run-p client:watch",
    "dev:debug": "NODE_ENV=development run-p client:watch:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,md,json,config.js,test.js}\"",
    "git:hooks": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "lint": "run-s lint:js lint:scss",
    "lint:fix": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:scss": "stylelint  \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "preview": "NODE_ENV=production node --use-strict .",
    "security-audit": "npm audit --audit-level=high",
    "server:debug": "node --watch --inspect ./src",
    "server:watch": "node --watch ./src",
    "prestart": "npm run build",
    "start": "npm run dev",
    "start:debug": "npm run dev:debug",
    "pretest": "npm run test:build:client",
    "test": "TZ=UTC vitest --coverage",
    "test:build:client": "NODE_ENV=test webpack",
    "test:watch": "npm run test"
  },
  "imports": {
    "#server/*": "./src/server/*"
  },
