{
  "name": "cdp-portal-frontend",
  "version": "0.163.0",
  "description": "CDP Portal Frontend",
  "main": "src/index.js",
  "engines": {
    "node": ">=20.11.1"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "scripts": {
    "build": "run-s build:client build:server",
    "build:client": "webpack --config webpack.config.cjs",
    "client:watch": "webpack --config webpack.config.cjs --watch",
    "build:server": "babel --extensions .js --delete-dir-on-start --ignore '**/*.test.js' --ignore 'src/__fixtures__' --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "docker:dev": "NODE_ENV=development run-p client:watch server:watch",
    "predev": "npm run build:client",
    "dev": "NODE_ENV=development run-p client:watch server:watch",
    "dev:debug": "NODE_ENV=development run-p client:watch server:debug",
    "format": "prettier --write 'src/**/*.js' '**/*.{js,cjs,ts,md,json,config.js,test.js}'",
    "format:check": "prettier --check 'src/**/*.js' '**/*.{js,cjs,ts,md,json,config.js,test.js}'",
    "postinstall": "npm run setup:husky",
    "lint": "run-s lint:js lint:scss",
    "lint:fix": "eslint --cache --fix .",
    "lint:js": "eslint --cache .",
    "lint:scss": "stylelint 'src/**/*.scss' --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "test": "TZ=UTC jest --coverage --verbose",
    "test:watch": "TZ=UTC jest --watch",
    "type:check": "tsc --build tsconfig.json",
    "server:watch": "nodemon --exec babel-node --extensions .js ./src",
    "server:debug": "nodemon --exec babel-node --extensions .js --inspect ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict ./.server",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "dependencies": {
    "@aws-sdk/client-firehose": "3.632.0",
    "@aws-sdk/client-s3": "3.651.1",
    "@aws-sdk/credential-providers": "3.632.0",
    "@babel/runtime": "7.24.7",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/basic": "7.0.2",
    "@hapi/bell": "13.0.2",
    "@hapi/boom": "10.0.1",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/cookie": "12.0.1",
    "@hapi/crumb": "9.0.1",
    "@hapi/hapi": "21.3.10",
    "@hapi/inert": "7.1.0",
    "@hapi/jwt": "3.2.0",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.1.0",
    "@hapi/yar": "11.0.2",
    "@smithy/property-provider": "3.1.1",
    "babel-plugin-module-resolver": "5.0.2",
    "bcrypt": "5.1.1",
    "convict": "6.2.4",
    "date-fns": "3.6.0",
    "disinfect": "2.0.0",
    "get-form-data": "3.0.0",
    "govuk-frontend": "5.4.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "history": "5.3.0",
    "https-proxy-agent": "7.0.4",
    "ioredis": "5.4.1",
    "joi": "17.13.1",
    "lodash": "4.17.21",
    "marked": "14.1.2",
    "marked-alert": "2.1.0",
    "marked-gfm-heading-id": "4.1.0",
    "node-fetch": "^2.7.0",
    "nunjucks": "3.2.4",
    "pino": "9.2.0",
    "pino-pretty": "11.2.1",
    "qs": "6.12.1",
    "undici": "6.19.2",
    "useragent": "2.3.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/node": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@tsconfig/node20": "20.1.4",
    "@types/convict": "6.1.6",
    "@types/eslint": "9.6.0",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.7",
    "@types/node": "22.3.0",
    "@types/nunjucks": "3.2.6",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "babel-jest": "29.7.0",
