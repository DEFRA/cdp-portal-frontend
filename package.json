{
  "name": "cdp-portal-frontend",
  "version": "1.0.0",
  "description": "CDP Portal Frontend",
  "sideEffects": false,
  "main": ".server/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "scripts": {
    "build": "run-s build:client build:server",
    "build:client": "NODE_ENV=production webpack",
    "build:server": "NODE_ENV=production babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"src/__fixtures__\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "dev": "run-p client:watch server:watch",
    "dev:debug": "NODE_ENV=development run-p client:watch server:debug",
    "docker:dev": "NODE_ENV=development run-p client:watch server:watch",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,cjs,ts,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,cjs,ts,md,json,config.js,test.js}\"",
    "client:watch": "NODE_ENV=development webpack --watch",
    "lint": "run-s lint:js lint:scss lint:types",
    "lint:fix": "eslint --cache --fix .",
    "lint:js": "eslint --cache .",
    "lint:types": "tsc --build tsconfig.json",
    "lint:scss": "stylelint  \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "postinstall": "npm run setup:husky",
    "predev": "npm run build:client",
    "prestart": "npm run build",
    "server:debug": "nodemon --exec tsx --enable-source-maps --inspect ./src",
    "server:watch": "nodemon --exec tsx --enable-source-maps ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "NODE_ENV=production node --use-strict .",
    "test": "TZ=UTC jest --colors --coverage --verbose",
    "test:watch": "TZ=UTC jest --colors --watch",
    "type:check": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-firehose": "3.682.0",
    "@aws-sdk/client-s3": "3.682.0",
    "@aws-sdk/credential-providers": "3.682.0",
    "@babel/runtime": "7.26.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/basic": "7.0.2",
    "@hapi/bell": "13.0.2",
    "@hapi/boom": "10.0.1",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/cookie": "12.0.1",
    "@hapi/crumb": "9.0.1",
    "@hapi/hapi": "21.3.12",
    "@hapi/hoek": "11.0.6",
    "@hapi/inert": "7.1.0",
    "@hapi/jwt": "3.2.0",
    "@hapi/scooter": "7.0.0",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.1.0",
    "@hapi/yar": "11.0.2",
    "@smithy/property-provider": "3.1.8",
    "babel-plugin-module-resolver": "5.0.2",
    "bcrypt": "5.1.1",
    "blankie": "5.0.0",
    "convict": "6.2.4",
    "date-fns": "4.1.0",
    "disinfect": "2.0.0",
    "get-form-data": "3.0.0",
    "govuk-frontend": "5.7.1",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "history": "5.3.0",
    "https-proxy-agent": "7.0.5",
    "ioredis": "5.4.1",
    "joi": "17.13.3",
    "lodash": "4.17.21",
    "marked": "14.1.3",
    "marked-alert": "2.1.1",
    "node-fetch": "^3.3.2",
    "nunjucks": "3.2.4",
    "pino": "9.5.0",
    "pino-pretty": "11.3.0",
    "qs": "6.13.0",
    "semver": "7.6.3",
    "undici": "6.20.1",
    "uuid": "11.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.26.0",
    "@babel/plugin-syntax-import-attributes": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@tsconfig/node20": "20.1.4",
    "@types/convict": "6.1.6",
    "@types/eslint": "9.6.1",
    "@types/jest": "29.5.14",
