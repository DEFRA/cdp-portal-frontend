{{ appPageHeading({
  caption: "Error",
  text: heading,
  intro: message
}) }}

{% if isAdmin and stack %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-full-width">
      <pre class="app-code-container">
        <code class="app-code">
          {{ stack | safe }}
        </code>
      </pre>
    </div>
  </div>
{% endif %}
